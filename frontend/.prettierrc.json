{
	"singleQuote": true,
	"semi": true,
	"printWidth": 100,
	"trailingComma": "none",
	"useTabs": true,
	"tabWidth": 4,
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindFunctions": ["clsx", "tw", ".*Styles"],
	"overrides": [
		{
			"files": "*.svg",
			"options": {
				"parser": "html"
			}
		}
	]
}